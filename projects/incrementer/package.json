{
  "name": "@substrate/contract-incrementer",
  "version": "0.0.1",
  "description": "Example AS-Substrate incrementer contract",
  "author": "Stefanie Doll",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "license": "Apache-2.0",
  "scripts": {
    "build:optimized": "asc assembly/index.ts -b build/contract-incrementer.wasm -t build/contract-incrementer.wat --importMemory --noAssert --use abort= --optimize",
    "build:sh": "./build.sh",
    "build:tsd": "asc assembly/index.ts -d build/index.d.ts",
    "build": "yarn build:optimized && yarn build:tsd && yarn build:sh"
  },
  "dependencies": {
    "as-bignum": "^0.2.2",
    "assemblyscript": "0.12.3"
  },
  "devDependencies": {
    "@substrate/as-types": "0.0.1",
    "@substrate/as-utils": "0.0.1",
    "@as-pect/cli": "^2.7.0"
  }
}
